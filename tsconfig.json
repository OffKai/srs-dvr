{
	"compilerOptions": {
		"alwaysStrict": true,
		"lib": ["ESNext"],
		"module": "Node16",
		"moduleResolution": "Node16",
		"sourceMap": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "ESNext",
		"verbatimModuleSyntax": true,
		"rootDir": "src",
		"outDir": "dist",
		"noEmit": true,
		"types": ["vitest/globals"]
	},
	"include": ["src"]
}
